Social Social::RS(){
    Social temp;
    temp.nSetA=nSetA;
    temp.nSetB=nSetB;
    temp.R = R;
    for (int i=0;i<temp.nSetA;i++){
        for (int j=0;j<nSetB;j++){
            if (R[i][j]==true){
                temp.R[j][i]=true;
            }
        }
    }
    return temp;
}

Social Social::Composition(Social& s){
    Social temp;
    temp.nSetA=nSetA;
    temp.nSetB=nSetB;
    temp.R = new bool*[nSetA];
    for (int i=0;i<temp.nSetA;i++){
        temp.R[i] = new bool[temp.nSetB];
        for (int j=0;j<temp.nSetB;j++){
            temp.R[i][j]=false;
        }
    }
    for (int i=0;i<temp.nSetA;i++){
        for (int j=0;j<temp.nSetB;j++){
            if (s.R[i][j]==true){
                for (int z=0;z<nSetB;z++){
                    if (R[j][z]==true){
                        temp.R[i][z]=true;
                    }
                }
            }
        }
    }
    return temp;
}

Social Social::Inverse(){
    Social temp;
    temp.nSetA=nSetA;
    temp.nSetB=nSetB;
    temp.R = new bool*[nSetA];
    for (int i=0;i<temp.nSetA;i++){
        temp.R[i] = new bool[temp.nSetB];
        for (int j=0;j<temp.nSetB;j++){
            temp.R[i][j]=false;
        }
