Social Social::operator+(const Social &s){
    Social temp;
    temp.nSetA=nSetA;
    temp.nSetB=nSetB;
    temp.R = new bool*[nSetA];
    for (int i=0;i<temp.nSetA;i++){
        temp.R[i] = new bool[temp.nSetB];
        for (int j=0;j<temp.nSetB;j++){
            if (s.R[i][j]==true||R[i][j]==true){
                temp.R[i][j]=true;
            }
