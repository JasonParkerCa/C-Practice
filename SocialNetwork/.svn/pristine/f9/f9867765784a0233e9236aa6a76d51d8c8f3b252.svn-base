}

Social Social::operator-(const Social &s){
    Social temp;
    temp.nSetA=nSetA;
    temp.nSetB=nSetB;
    temp.R = R;
    for (int i=0;i<temp.nSetA;i++){
        for (int j=0;j<temp.nSetB;j++){
            if (s.R[i][j]){
                temp.R[i][j]=false;
            }
        }
