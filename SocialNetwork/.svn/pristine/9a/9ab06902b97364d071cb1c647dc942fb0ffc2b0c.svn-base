int** makeBinary(){
    int numberofPowerset, temp;
    numberofPowerset=pow(2,NumberofU);
    int** BV = new int* [numberofPowerset];
    for (int i=0;i<numberofPowerset;i++){
        BV[i] = new int [NumberofU];
    }
    for (int i=0;i<numberofPowerset;i++){
        temp=i;
        for (int j=NumberofU-1;j>=0;j--){
            BV[i][j]=temp%2;
            temp/=2;
        }
    }
    for (int i=0;i<numberofPowerset;i++){
        for (int j=0;j<NumberofU;j++){
            cout << BV[i][j];
        }
        cout << endl;
    }
    return BV;
}

